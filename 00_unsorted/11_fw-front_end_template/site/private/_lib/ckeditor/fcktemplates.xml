<Templates imagesBasePath="fck_template/images/">

	<!-- DIVIDERS -->
	<Template title="Solid divider" image="tmpl-divider-solid.gif">
		<Description>Inserts a red solid divider</Description>
		<Html>
			<![CDATA[
			<div class="divider dividerSolid">&nbsp;</div>
			]]>
		</Html>
	</Template>
	
	<Template title="Graphical divider" image="tmpl-divider-graphic.gif">
		<Description>Inserts a graphical divider</Description>
		<Html>
			<![CDATA[
			<div class="divider dividerGraphic">&nbsp;</div>
			]]>
		</Html>
	</Template>
	
	<Template title="Lite divider" image="tmpl-divider-lite.gif">
		<Description>Inserts a lite divider</Description>
		<Html>
			<![CDATA[
			<div class="divider dividerLite">&nbsp;</div>
			]]>
		</Html>
	</Template>
	
	<Template title="Dotted divider" image="tmpl-divider-dotted.gif">
		<Description>Inserts a dotted divider</Description>
		<Html>
			<![CDATA[
			<div class="divider dividerDotted">&nbsp;</div>
			]]>
		</Html>
	</Template>	
	
	<!-- HIGHLIGHT BOXES -->
	<Template title="Highlight text block" image="tmpl-box-highlightbox.gif">
		<Description>Inserts a highlighted block of text</Description>
		<Html>
			<![CDATA[
			<div class="blockTextHighlight">
				<p>[Content here ...]</p>
			</div>
			<p>&nbsp;</p>
			]]>
		</Html>
	</Template>
	
	<!-- SPECIAL BOXES -->
	<Template title="Special link box" image="tmpl-box-link.gif">
		<Description>Inserts a special styled box for a link and its description</Description>
		<Html>
			<![CDATA[
			<div class="specialBox link">
				<p><a href="#">[link title]</a> <span class="note">([link note])</span><br />[Description for link]</p>
			</div>
			<p>&nbsp;</p>			
			]]>
		</Html>
	</Template>	
	
	<Template title="Special Download box" image="tmpl-box-dl.gif">
		<Description>Inserts a special styled box for a download item and its description</Description>
		<Html>
			<![CDATA[
			<div class="specialBox dl">
				<p><a href="#">[URL or title]</a> <span class="note">([note, e.g. Filesize, Format])</span><br />[Description for download item]</p>
			</div>
			<p>&nbsp;</p>
			]]>
		</Html>
	</Template>
	
	<!-- TABLE -->
	<Template title="Table (two-column)" image="tmpl-table-styled.gif">
		<Description>Inserts a two-column table with table headings in default styles.</Description>
		<Html>
			<![CDATA[
			<table width="100%" border="0" cellspacing="0" cellpadding="0" class="styledTable">
				<tr class="noBorder">
					<th>[column 1 heading]</th>
					<th>[column 2 heading]</th>
				</tr>
				<tr>
					<td>[cell content]</td>
					<td>[cell content]</td>
				</tr>
				<tr>
					<td>[cell content]</td>
					<td>[cell content]</td>
				</tr>
				<tr>
					<td>[cell content]</td>
					<td>[cell content]</td>
				</tr>
			</table>
			<p>&nbsp;</p>
			]]>
		</Html>
	</Template>
	
	<Template title="Table (Layout Mode)" image="tmpl-table-layout.gif">
		<Description>Inserts a table with no borders, margins, cell-padding, or cell-spacing. Ideal for laying-out content.</Description>
		<Html>
			<![CDATA[
			<table class="layoutMode">
				<tr>
					<td>[cell]</td>
					<td>[cell]</td>
				</tr>
				<tr>
					<td>[cell]</td>
					<td>[cell]</td>
				</tr>
				<tr>
					<td>[cell]</td>
					<td>[cell]</td>
				</tr>
			</table>
			<p>&nbsp;</p>
			]]>
		</Html>
	</Template>
	
